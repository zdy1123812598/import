create table CQXX_LOG
(
  recid   INTEGER,
  keycode VARCHAR2(200),
  msg     VARCHAR2(1000),
  type_id INTEGER,
  isgd    INTEGER,
  t_user  VARCHAR2(100),
  sjyy    VARCHAR2(100),
  tag     INTEGER,
  sffwxx  INTEGER
)
;
comment on column CQXX_LOG.isgd
  is '0：未归档；1：已归档';
comment on column CQXX_LOG.sffwxx
  is '是否存在房屋信息';

