create table SPFYG_LOG
(
  recid   INTEGER,
  keycode VARCHAR2(200),
  msg     VARCHAR2(1000),
  type_id INTEGER,
  isgd    INTEGER,
  t_user  VARCHAR2(100),
  sjyy    VARCHAR2(100),
  tag     INTEGER,
  isgx    INTEGER,
  sshtxx  INTEGER,
  ssygxx  INTEGER
)
;
comment on column SPFYG_LOG.isgx
  is '是否更新';
comment on column SPFYG_LOG.sshtxx
  is '上手合同信息是否存在';
comment on column SPFYG_LOG.ssygxx
  is '上手预告信息是否存在';

